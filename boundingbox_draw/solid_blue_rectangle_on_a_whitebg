# from google.colab.patches import *
import numpy as np
import cv2

image = np.zeros([512,512,3],dtype=np.uint8)
image.fill(255)
# window_name = 'Image'
start_point = (5, 5)
end_point = (220, 220)
color = (0, 255, 0)
thickness = -1
image = cv2.rectangle(image, start_point, end_point, color, thickness)
# cv2_imshow(image)
cv2.imshow("Image", image)
cv2.waitKey(0)
cv2.destroyAllWindows()